      "actions": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "gradeActionCode": {
              "type": "string",
              "description": "Grade action code.",
              "example": "04"
            },
            "gradeActionDescription": {
              "type": "string",
              "description": "Description of the grade action.",
              "example": "Complete assignments"
            }
          }
        }
      },
      "reasons": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "gradeReasonCode": {
              "type": "string",
              "description": "Grade reason code.",
              "example": "03"
            },
            "gradeReasonDescription": {
              "type": "string",
              "description": "Description of the grade reason.",
              "example": "Missed assignments/assessments"
            }
          }
        }
      }
    }
  }
