{
  "type": "object",
  "properties": {
    "acadCareer": {
      "type": "string",
      "description": "The academic career code.",
      "example": "GRAD"
    },
    "termDescr": {
      "type": "string",
      "description": "The term description.",
      "example": "2000 Spring"
    },
    "careerDescr": {
      "type": "string",
      "description": "The career description.",
      "example": "Graduate"
    },
    "termBeginDate": {
      "type": "integer",
      "description": "Timestamp for the term begin date.",
      "example": 948092400000
    },
    "termEndDate": {
      "type": "integer",
      "description": "Timestamp for the term end date.",
      "example": 958028400000
    },
    "gradingScheme": {
      "type": "string",
      "description": "The grading scheme.",
      "example": "GRD"
    },
    "sessionMap": {
      "type": "object",
      "description": "Map of sessions for the career.",
      "properties": {
        "REG": {
          "$ref": "session.json"
        }
      }
    }
  }
}
