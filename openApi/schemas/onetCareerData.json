{
  "type": "object",
  "properties": {
    "onetOccupationOverride": {
      "type": "string",
      "description": "The ONET occupation override.",
      "example": "CEO"
    },
    "title": {
      "type": "string",
      "description": "The job title.",
      "example": "Computer and Information Research Scientists"
    },
    "alternateTitles": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "description": "Alternate job titles.",
      "example": [
        "Artificial Intelligence Specialist",
        "Bioinformatics Software Developer",
        "Computational Linguist"
      ]
    },
    "tags": {
      "type": "object",
      "properties": {
        "brightOutlook": {
          "type": "boolean",
          "description": "Indicates if the job has a bright outlook.",
          "example": true
        },
        "green": {
          "type": "boolean",
          "description": "Indicates if the job is green.",
          "example": true
        }
      },
      "description": "Tags associated with the job."
    },
    "tasks": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "description": "Tasks associated with the job.",
      "example": [
        "Develop new uses for known materials",
        "Create new methods for the analysis of data"
      ]
    },
    "knowledge": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "description": "Name of the knowledge.",
            "example": "Computers and Electronics"
          },
          "description": {
            "type": "string",
            "description": "Description of the knowledge.",
            "example": "Knowledge of circuit boards, processors, chips, electronic equipment, and computer hardware and software, including applications and programming."
          }
        }
      },
      "description": "Knowledge related to the job."
    },
    "abilities": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "description": "Name of the ability.",
            "example": "Deductive Reasoning"
          },
          "description": {
            "type": "string",
            "description": "Description of the ability.",
            "example": "The ability to apply general rules to specific problems to produce answers that make sense."
          }
        }
      },
      "description": "Abilities related to the job."
    },
    "usWageData": {
      "type": "object",
      "properties": {
        "hourlyMean": {
          "type": "number",
          "description": "Hourly mean wage.",
          "example": 53.0
        },
        "hourlyPct10": {
          "type": "number",
          "description": "Hourly 10th percentile wage.",
          "example": 29.0
        },
        "hourlyPct25": {
          "type": "number",
          "description": "Hourly 25th percentile wage.",
          "example": 37.0
        }
      },
      "description": "US wage data."
    },
    "azWageData": {
      "type": "object",
      "properties": {
        "hourlyMean": {
          "type": "number",
          "description": "Arizona hourly mean wage.",
          "example": 52.0
        },
        "hourlyPct10": {
          "type": "number",
          "description": "Arizona hourly 10th percentile wage.",
          "example": 29.0
        },
        "hourlyPct25": {
          "type": "number",
          "description": "Arizona hourly 25th percentile wage.",
          "example": 36.0
        }
      },
      "description": "Arizona wage data."
    },
    "employmentData": {
      "type": "object",
      "properties": {
        "employment": {
          "type": "number",
          "description": "Employment count.",
          "example": 1000
        },
        "employmentChange": {
          "type": "number",
          "description": "Employment change count.",
          "example": 100
        }
      },
      "description": "Employment data."
    },
    "wageYear": {
      "type": "integer",
      "description": "The year for wage data.",
      "example": 2020
    },
    "employmentYear": {
      "type": "integer",
      "description": "The year for employment data.",
      "example": 2020
    }
  }
}
