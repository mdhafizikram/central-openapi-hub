{
  "openapi": "3.0.0",
  "info": {
    "title": "ASU Course Catalog API",
    "description": "Our API allows users to effortlessly explore a wide range of courses offered by various Colleges and Schools at Arizona State University. With intuitive endpoints and comprehensive filters, users can easily navigate through different academic programs, view course details, and access essential information, empowering them to make informed decisions about their educational journey at ASU.",
    "contact": {
      "email": "customerservice@asu.edu"
    },
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "https://api.myasuplat-dpl.asu.edu"
    }
  ],
  "tags": [
    {
      "name": "Courses",
      "description": "Operations related to courses"
    }
  ],
  "paths": {
    "/api/course/{strm}/{courseId}/{courseOfferNumber}": {
      "get": {
        "tags": ["Courses"],
        "summary": "Get course details by CourseID, term and offer number",
        "description": "Returns details of a specific course for a given term, courseId , courseOfferingNumber.",
        "parameters": [
          {
            "name": "include",
            "in": "query",
            "description": "Include additional information like equivalent courses.",
            "schema": {
              "type": "string"
            },
            "example": "equivalentCourses"
          },
          {
            "name": "strm",
            "in": "path",
            "required": true,
            "description": "The term code.",
            "schema": {
              "type": "integer"
            },
            "example": "2247"
          },
          {
            "name": "courseId",
            "in": "path",
            "required": true,
            "description": "The course ID.",
            "schema": {
              "type": "integer"
            },
            "example": "104171"
          },
          {
            "name": "courseOfferNumber",
            "in": "path",
            "required": true,
            "description": "The course offer number.",
            "schema": {
              "type": "integer"
            },
            "example": "1"
          }
        ],
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Course"
                }
              }
            }
          },
          "404": {
            "description": "Course not found",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "error": {
                      "type": "string",
                      "description": "Error message for course not found.",
                      "example": "Course not found."
                    }
                  }
                }
              }
            }
          },
          "500": {
            "description": "Internal server error",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "error": {
                      "type": "string",
                      "description": "Error message for internal server error.",
                      "example": "Internal server error."
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/api/class/{strm}/subject/{subject}/{catalogNumber}": {
      "get": {
        "tags": ["Courses"],
        "summary": "Get class details by term, subject, and catalog number",
        "description": "Returns details of classes for a given term, subject, and catalog number.",
