              "type": "integer"
            },
            "example": 2247
          },
          {
            "name": "subject",
            "in": "path",
            "required": true,
            "description": "The subject of the course.",
            "schema": {
              "type": "string"
            },
            "example": "CSE"
          },
          {
            "name": "catalogNumber",
            "in": "path",
            "required": true,
            "description": "The catalog number of the course.",
            "schema": {
              "type": "integer"
            },
            "example": 100
